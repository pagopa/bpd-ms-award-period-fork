FROM adoptopenjdk/openjdk11:alpine-jre
ADD https://github.com/microsoft/ApplicationInsights-Java/releases/download/3.1.0/applicationinsights-agent-3.1.0.jar /applicationinsights-agent.jar
COPY docker/applicationinsights.json ./applicationinsights.json
VOLUME /tmp
COPY target/*.jar app.jar
ENTRYPOINT ["java","-jar","app.jar"]
